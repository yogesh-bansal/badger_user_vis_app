## Price Match#
price_match_sett <- function(date_m,sett_hist)#
{#
	match_idx <- match(date_m,as_date(sett_hist$Time))#
	if(is.na(match_idx)) return(NA)#
	as.numeric(sett_hist$value[match_idx[1]])#
}#
