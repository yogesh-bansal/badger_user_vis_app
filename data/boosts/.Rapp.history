  scale_y_continuous(trans=log10_trans(),breaks=c(c(1,1.2,1.5,1.8,2,2.5,3:13)*10^8),labels=c(paste0("$",100*c(1,1.2,1.5,1.8,2,2.5,3:13),"M")))+#
  xlab('Date') +#
  ylab('Vault Volume')+#
  theme(axis.text.x = element_text(angle = 90))#
